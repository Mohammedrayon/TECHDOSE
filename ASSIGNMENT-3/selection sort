class Solution:
    def sortArray(self, nums: List[int]) -> List[int]:
        n=len(nums)
        for i in range(n):
            m=i
            for j in range(i+1,n):
                if(nums[m]>nums[j]):
                    m=j
            if(m!=i):
                temp=nums[m]
                nums[m]=nums[i]
                nums[i]=temp
        return nums                
        
